{
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "mimeTypes": {
    ".css": "text/css",
    ".js": "application/javascript",
    ".wasm": "application/wasm",
    ".dll": "application/octet-stream"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  }
}