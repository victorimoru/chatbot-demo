{
  "routes": [
    {
      "route": "/_framework/*",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      }
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "mimeTypes": {
    ".css": "text/css",
    ".js": "application/javascript",
    ".wasm": "application/wasm",
    ".dll": "application/octet-stream",
    ".json": "application/json",
    ".html": "text/html"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  }
}