{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/images/*",
      "/css/*",
      "/js/*",
      "/Xenova/*",
      "/en/Xenova/*"
    ]
  },
  "routes": [
    {
      "route": "/_framework/*",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      }
    },
    {
      "route": "/css/*",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      }
    },
    {
      "route": "/js/*",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      }
    }
  ]
}
